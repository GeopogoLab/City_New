# Required Google Maps Platform key with Tiles API + Elevation API access
VITE_GOOGLE_MAPS_API_KEY=

# When true, auto centers the camera on the loaded photorealistic tileset
VITE_AUTO_CENTER_TILESET=false

# Optional: Mapbox token for geocoding (search bar). If set, Mapbox geocoder is used; otherwise falls back to Google Geocoding.
VITE_MAPBOX_ACCESS_TOKEN=
